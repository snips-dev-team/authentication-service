    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "nodemon": "^2.0.7",
    "sucrase": "^3.17.1",
